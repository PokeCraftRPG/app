{
  "Account": {
    "AllocatedBytes": 5242880, // 5 MB
    "WorldLimit": 3
  },
  "AllowedHosts": "*",
  "Caching": {
    "ActorLifetime": "00:15:00"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenAuthentication": {
    "AccessToken": {
      "TokenType": "at+jwt",
      "LifetimeSeconds": 300
    }
  },
  "Portal": {
    "Realm": "pokemon"
  }
}
